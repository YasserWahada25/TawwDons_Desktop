<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.web.WebView?>

<BorderPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.event.controller.MapPopupController"
            prefWidth="800"
            prefHeight="600">

    <center>
        <WebView fx:id="webView" prefWidth="800" prefHeight="550"/>
    </center>

    <bottom>
        <HBox alignment="CENTER" spacing="20" style="-fx-padding: 10;">
            <Button text="Valider" onAction="#handleValidate"
                    style="-fx-font-size: 14px; -fx-pref-width: 120px;"/>
        </HBox>
    </bottom>

</BorderPane>